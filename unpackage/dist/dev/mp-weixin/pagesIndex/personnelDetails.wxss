.details {
  padding: 20px;
}
.margin-l {
  margin-left: 20px;
}
.img-1 {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  margin-bottom: 10px;
}
.img-1 image {
  width: 110px;
  height: 110px;
}
.img-1 .img-item {
  width: 32%;
}
.img-1 .img-item:last-child:nth-child(3n + 2) {
  margin-right: calc((100% - 31%) / 2);
}
.u-form {
  margin: 20px 0;
}

